.lib smps_cb.lib
;.OPTIONS ITL5=1000 ITL4=40 METHOD=GEAR
.OPTIONS METHOD=GEAR reltol=.01
**** Generic power switch model ****
.MODEL SW1 VSWITCH (RON=10M VON=10 VOFF=1 ROFF=1E6)
**** Free wheel diode model ****
.MODEL DN5829 D (IS=5.71E-04 N=1.25 BV=2.66E+01 IBV=8.5M
+ RS=3.75E-03 CJO=8.19E-09 VJ=.125 M=.426 XTI=2 EG=0.69 TT=1.80E-11)

**** Generic PWM controller parameters ****
.PARAM REF = 2.5V				; internal reference voltage
.PARAM PERIOD = 5U			; switching period
.PARAM RATIO = 1				; Max peak current versus err. amp level
.PARAM DUTYMAX = 0.8			; maximum duty cycle
.PARAM RAMP = 5V				; external ramp for compensation
.PARAM VOUTHI = 15V			; driver output voltage high
.PARAM VOUTLO = 150M			; driver output voltage low
.PARAM ROUT = 10				; driver output resistor
**** Internal error amplifier parameters ****
.PARAM VHIGH = 2.8			; maximum output voltage
.PARAM ISINK = 15M			; Sink capability
.PARAM ISOURCE = 500U		; Source capability
.PARAM VLOW = 100M			; Minimum output voltage
.PARAM POLE = 30				; First pole in Hertz
.PARAM GAIN = 31622			; DC open-loop gain (default=90dB)


