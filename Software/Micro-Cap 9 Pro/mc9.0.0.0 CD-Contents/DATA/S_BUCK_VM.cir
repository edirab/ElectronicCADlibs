.lib smps_cb.lib
.OPTIONS method=gear
.MODEL S11 VSWITCH (RON=1M ROFF=25)
.MODEL PULSE1 PUL (VZERO=10 VONE=0 P1=2100U P2=2100U P3=2800U P4=2800U P5=5M)

***** DEVICE MODELS ****
.MODEL DN5817 D BV=2.66E+01 CJO=2.31E-10 EG=0.69
+ IBV=3.00E-04 IS=3.53E-05 M=.588 N=1.30 RS=4.47E-02
+ TT=7.20E-11 VJ=2.42 XTI=2

**** Generic PWM controller parameters ****
.PARAM REF = 1.2V				; internal reference voltage
.PARAM PERIOD = 5U			; switching period
.PARAM DUTYMAX = 0.99		; maximum duty cycle
.PARAM DUTYMIN = 0.01		; minimum duty cycle
.PARAM IMAX = 2.5V			; max voltage upon (external) RSENSE resistor 
.PARAM VOUTHI = 15V			; driver output voltage high
.PARAM VOUTLO = 120M			; driver output voltage low
.PARAM ROUT = 10				; driver output resistor
**** Internal error amplifier parameters ****
.PARAM VHIGH = 3V				; maximum output voltage
.PARAM VLOW = 300M			; minimum output voltage
.PARAM ISINK = 15M			; sink capability
.PARAM ISOURCE = 100u		; source capability
.PARAM POLE = 30				; first pole in Hertz
.PARAM GAIN = 31622			; DC open-loop gain (default=90dB)
********************

***** MOSFET MODEL *****
.SUBCKT IRFP044N 1 2 3
M1 9 7 8 8 MM L=100u W=100u
.MODEL MM NMOS LEVEL=1 IS=1e-32
+ VTO=3.81531 LAMBDA=0 KP=35.6687
+ CGSO=1.40569e-05 CGDO=3.21208e-07
RS 8 3 0.0143504
D1 3 1 MD
.MODEL MD D IS=6.65214e-09 RS=0.00681888 N=1.44936 BV=55
+ IBV=0.00025 EG=1 XTI=2.81713 TT=0.0001
+ CJO=1.32544e-09 VJ=1.29661 M=0.493619 FC=0.5
RDS 3 1 2.2e+06
RD 9 1 0.0001
RG 2 7 4.21415
D2 4 5 MD1
.MODEL MD1 D IS=1e-32 N=50
+ CJO=1.5055e-09 VJ=0.5 M=0.677697 FC=1e-08
D3 0 5 MD2
.MODEL MD2 D IS=1e-10 N=0.426578 RS=3e-06
RL 5 10 1
FI2 7 9 VFI2 -1
VFI2 4 0 0
EV16 10 0 9 7 1
CAP 11 10 1.86655e-09
FI1 7 9 VFI1 -1
VFI1 11 6 0
RCAP 6 10 1
D4 0 6 MD3
.MODEL MD3 D IS=1e-10 N=0.426578
.ENDS IRFP044N


